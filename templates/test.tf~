
# A Vulnhub machine on the network
resource "aws_instance" "djinn" {
  ami                    = "ami-0a1dee5086856c399" # Custom AMI, uploaded using https://docs.amazonaws.cn/en_us/vm-import/latest/userguide/vm-import-ug.pdf
  instance_type          = var.instance_type
  key_name               = "primary"
  subnet_id              = aws_subnet.my_subnet.id
  vpc_security_group_ids = [aws_security_group.allow_vuln.id]

  tags = {
    Name = "djinn"
  }
}
# Don't change the name of the output, will break Webapp :)
output "djinn" {
  value = aws_instance.djinn.private_ip
}

